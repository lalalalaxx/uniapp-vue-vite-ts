        "sass-loader": "^10.4.1",
        "typescript": "^4.9.4",
        "unplugin-auto-import": "^0.16.6",
        "unplugin-vue-components": "^0.25.1",
        "vite": "4.0.4",
        "vue-tsc": "^1.0.24"
    }
